{
    "env": {
        "production": {
            "presets": [
                "latest",
            ],
            "plugins": [
                "add-module-exports",
                "transform-es2015-modules-amd",
                "transform-runtime",
            ],
        },
        "development": {
            "presets": [
                "latest",
            ],
            "plugins": [
                "add-module-exports",
                "transform-es2015-modules-amd",
                "transform-runtime",
            ],
        },
        "test": {
            "presets": [
                ["env", {
                    "targets": {
                        "node": "current"
                    }
                }]
            ],
            "plugins": [
                "transform-amd-to-commonjs",
                ["import-globals", { "sinon": "sinon" }]
            ]
        },
        "internal": {
            "presets": [
                ["env", {
                    "targets": {
                        "browsers": [
                            "last 2 Chrome versions",
                            "last 1 Safari version",
                            "last 2 Firefox versions",
                            "last 2 Edge versions",
                        ],
                    }
                }],
            ],
        },
    },
    "ignore": [
        "eslintrc.js",
    ],
}
